					"script": {
						"exec": [
							"pm.test(\"TC01 - Request body exists\", function () {\r",
							"    pm.expect(pm.request.body).to.exist;\r",
							"});\r",
							"pm.test(\"TC02 - Body mode is raw\", function () {\r",
							"    pm.expect(pm.request.body.mode).to.eql(\"raw\");\r",
							"});\r",
							"pm.test(\"TC03 - Username field exists in request\", function () {\r",
							"    let req = JSON.parse(pm.request.body.raw);\r",
							"    pm.expect(req).to.have.property(\"username\");\r",
							"});\r",
							"pm.test(\"TC04 - Password field exists in request\", function () {\r",
							"    let req = JSON.parse(pm.request.body.raw);\r",
							"    pm.expect(req).to.have.property(\"password\");\r",
							"});\r",
							"pm.test(\"TC05 - Username is not empty\", function () {\r",
							"    let req = JSON.parse(pm.request.body.raw);\r",
							"    pm.expect(req.username).to.not.be.empty;\r",
							"});\r",
							"pm.test(\"TC06 - Password is not empty\", function () {\r",
							"    let req = JSON.parse(pm.request.body.raw);\r",
							"    pm.expect(req.password).to.not.be.empty;\r",
							"});\r",
							"pm.test(\"TC07 - Password not returned in response\", function () {\r",
							"    pm.expect(pm.response.text()).to.not.include(\"password\");\r",
							"});\r",
							"pm.test(\"TC08 - Username not returned in response\", function () {\r",
							"    pm.expect(pm.response.text()).to.not.include(\"username\");\r",
							"});\r",
							"pm.test(\"TC09 - Token does not contain username\", function () {\r",
							"    pm.expect(pm.response.json().token).to.not.include(\"admin\");\r",
							"});\r",
							"pm.test(\"TC10 - Token does not contain password\", function () {\r",
							"    pm.expect(pm.response.json().token).to.not.include(\"password\");\r",
							"});\r",
							"pm.test(\"TC11 - Single field in response body\", function () {\r",
							"    let bodyKeys = Object.keys(pm.response.json());\r",
							"    pm.expect(bodyKeys.length).to.eql(1);\r",
							"});\r",
							"pm.test(\"TC12 - Token key is lowercase\", function () {\r",
							"    let key = Object.keys(pm.response.json())[0];\r",
							"    pm.expect(key).to.eql(\"token\");\r",
							"});\r",
							"pm.test(\"TC13 - Token has no special characters\", function () {\r",
							"    pm.expect(pm.response.json().token).to.match(/^[a-zA-Z0-9]+$/);\r",
							"});\r",
							"pm.test(\"TC14 - Token value not null\", function () {\r",
							"    pm.expect(pm.response.json().token).to.not.be.null;\r",
							"});\r",
							"pm.test(\"TC15 - Multiple POST requests allowed\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200]);\r",
							"});\r",
							"pm.test(\"TC16 - Token value exists per request\", function () {\r",
							"    pm.expect(pm.response.json().token).to.exist;\r",
							"});\r",
							"pm.test(\"TC17 - Credentials trimmed by server\", function () {\r",
							"    pm.expect(pm.response.code).to.eql(200);\r",
							"});\r",
							"pm.test(\"TC18 - Missing username handled\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 400]);\r",
							"});\r",
							"pm.test(\"TC19 - Missing password handled\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 400]);\r",
							"});\r",
							"pm.test(\"TC20 - Invalid credentials do not return token\", function () {\r",
							"    if (pm.response.code !== 200) {\r",
							"        pm.expect(pm.response.text()).to.not.include(\"token\");\r",
							"    }\r",
							"});\r",
							"pm.test(\"TC21 - SQL injection attempt handled\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 400, 401]);\r",
							"});\r",
							"pm.test(\"TC22 - Empty JSON handled\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 400]);\r",
							"});\r",
							"pm.test(\"TC23 - Only POST method allowed\", function () {\r",
							"    pm.expect(pm.request.method).to.eql(\"POST\");\r",
